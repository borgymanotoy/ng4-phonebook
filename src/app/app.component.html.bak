<div *ngIf="items">
    <table border="1">
        <tr *ngFor="let item of items">
        <td>{{item.name}}</td>
        <td>{{item.price}}</td>
        <td>
            &nbsp;
            <img *ngIf="item.image" [src]='sanitizer.bypassSecurityTrustUrl(item.image.source)' [width]="item.image.width" [height]="item.image.height" alt="image-not-showing" />
        </td>
        </tr>
    </table>
</div>

